<table class="table table-striped table-responsive">
<thead>
     <div class="form-group">
            <button onclick="document.location='/employee'" class="btn btn-info"><i class="fa fa-database"></i>Add New Employee</button>
     </div>
</thead>
<thead>
    <tr>
        <th>Full Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>City</th>
        <th></th>
    </tr>
</thead>
<tbody>
    {{#each list}}
    <tr>
        <td>{{this.fullName}}</td>
        <td>{{this.email}}</td>
        <td>{{this.mobile}}</td>
        <td>{{this.city}}</td>
        <td><a href="{{this.id}}">Edit</a>
        <a href="delete/{{this.id}}">Delete</a></td>
    </tr>
    {{/each}}
</tbody>
</table>